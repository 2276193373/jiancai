.box {
    box-shadow: inset 0 0 2px 0 rgba(0,0,0, .23);
    width: 8px;
    height: 8px;
    position: absolute;
    top: 50%;
    transform: translate(-50%,-50%) rotateZ(45deg);
    background-color: #e9e9ee;
    z-index: 2;
}
.box-right {
    right: 0;
    transform: translate(50%,-50%) rotateZ(45deg);
    /*border: 1px solid red;*/
}
.sort-line {
    border-top: 1px solid #dbdbe0;
}
.line {
    border-top: 1px dashed #dbdbe0;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: 0px;
    width: 100%;
    height: 0;
    z-index: 1;
}
