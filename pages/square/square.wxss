/* 弹出层里面的文字 */
.text{
    text-align: justify;
    font-size: 14px;
    color: #666;
    width: 180px;
    margin-top: 10px;
}
.triangle {
    width: 30rpx;
    height: 30rpx;
    transform: translate(200rpx, -50%) rotateZ(45deg) ;
    background-color: white;
    position: absolute;
}
.type {
    width: 78rpx;
    height: 63rpx;
    background-color: #FF4E30;
    box-shadow: 0 4rpx 6rpx 0 rgba(73, 10, 10, .3);
    border-radius: 6rpx 6rpx 6rpx 0;
    font-size: 21px;
    color: #ffffff;
    text-align: center;
    line-height: 63rpx;
    position: absolute;
    right: -17rpx;
    top: -20rpx;
}
.solved {
    width: 25px;
    height: 62px;
    /*border: 1px solid red;*/
    position: absolute;
    right: 0;
    top: 0;
    writing-mode: vertical-lr;
    color: #ffffff;
    border-top-right-radius: 10px;
    overflow: hidden;
}
.solved > image {
    width: 25px;
    height: 62px;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 0;
}
.solved > text {
    position: absolute;
    z-index: 1;
    font-size: 12px;
    left: 50%;
    top: 10%;
    transform: translateX(-50%);
    letter-spacing: 2px;
}
